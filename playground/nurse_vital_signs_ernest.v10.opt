                <items id="description">A artéria radial direita.</items>
            </items>
            <items code="at1040">
                <items id="text">Coração</items>
                <items id="description">A região do coração.</items>
            </items>
            <items code="at1041">
                <items id="text">Artéria Carótida Esquerda</items>
                <items id="description">A artéria carótida esquerda.</items>
            </items>
            <items code="at1042">
