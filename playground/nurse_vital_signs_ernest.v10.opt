                <items id="text">Strukturerad mätplats</items>
                <items id="description">Strukturerad anatomisk plats där mätningen gjordes.</items>
            </items>
            <items code="at1058">
                <items id="text">Extra information</items>
                <items id="description">Ytterligare information som krävs för att fånga lokal
                    kontext eller för anpassning till andra referensmodeller och formalismer.</items>
                <items id="comment">Exempelvis uppgifter om en lokal sjukhusavdelning eller
                    ytterligare metadata för anpassning till FHIR- eller CIMI-motsvarigheter.</items>
            </items>
            <items code="at1059">
                <items id="text">Klinisk tolkning</items>
                <items id="description">Ord, fras eller kort beskrivning som representerar den
                    kliniska betydelsen och signifikansen av blodtrycksmätningen.</items>
            </items>
            <items code="mm[Hg]">
                <items id="text">mmHg</items>
                <items id="description">millimeters of mercury</items>
            </items>
