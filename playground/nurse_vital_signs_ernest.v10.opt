        <term_definitions language="pt-br">
            <items code="at0000">
                <items id="text">Pulso/Batimento Cardíaco</items>
                <items id="description">*The rate and associated attributes for a pulse or heart
                    beat. (en)</items>
            </items>
            <items code="at0001">
