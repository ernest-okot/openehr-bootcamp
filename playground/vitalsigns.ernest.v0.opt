                <term_definitions code="at0000.1">
                    <items id="text">Pulse</items>
                    <items id="description">A series of pressure waves observed in an artery, generated as the heart pumps blood through the circulatory system.</items>
                </term_definitions>
                <term_definitions code="at1005.1">
                    <items id="text">Presence</items>
                    <items id="description">Observation about the detection of an arterial pulse.</items>
                    <items id="comment">It can be implied that the pulse is present if Rate &gt;0 /min.</items>
                </term_definitions>
                <term_definitions code="at0004.1">
                    <items id="text">Pulse rate</items>
                    <items id="description">The measured rate of the pulse.</items>
                </term_definitions>
                <term_definitions code="at0005.1">
                    <items id="text">Regularity</items>
                    <items id="description">The regularity of the pulse.</items>
                    <items id="comment">This data element allows a choice of datatypes. Use the DV_TEXT data type to support inclusion of alternative or extended value sets.</items>
                </term_definitions>
                <term_definitions code="at1022.1">
                    <items id="text">Clinical description</items>
                    <items id="description">Narrative description about the pulse.</items>
                </term_definitions>
                <term_definitions code="at1023.1">
                    <items id="text">Clinical interpretation</items>
                    <items id="description">Single word, phrase or brief description that represents the clinical meaning and significance of the pulse findings, including the rhythm.</items>
                    <items id="comment">For example: 'bradycardia'; or 'ectopic beats'. Coding with a terminology is preferred, where possible. This data element has multiple occurrences to allow the recording of more than one 'Clinical interpretation'.</items>
                </term_definitions>
                <term_definitions code="at1059.1">
                    <items id="text">Comment</items>
                    <items id="description">Additional narrative about the pulse findings not captured in other fields.</items>
                </term_definitions>
                <term_definitions code="at1036.1">
                    <items id="text">Maximum pulse rate</items>
                    <items id="description">Maximum pulse rate observed during a specified time interval.</items>
                </term_definitions>
                <term_definitions code="at1019.1">
                    <items id="text">Method</items>
                    <items id="description">Method used to observe the pulse.</items>
                    <items id="comment">For example, palpation or pulse oximetry.</items>
                </term_definitions>
                <term_definitions code="at0.1">
                    <items id="text">Pulse oximetry</items>
                    <items id="description">The findings are observed using a pulse oximetry device.</items>
                </term_definitions>
                <term_definitions code="at0.2">
                    <items id="text">Doppler</items>
                    <items id="description">The findings are observed using a doppler ultrasound device.</items>
                </term_definitions>
                <term_definitions code="at0.3">
                    <items id="text">Oscillometry</items>
                    <items id="description">The findings are observed using an oscillometric device, such as an automatic blood pressure device.</items>
                </term_definitions>
                <term_definitions code="at0.4">
                    <items id="text">Intra-arterial probe</items>
                    <items id="description">The findings are observed using an intra-arterial probe, directly measuring the pressure wave in the artery.</items>
                </term_definitions>
                <term_definitions code="at1013.1">
                    <items id="text">Device</items>
                    <items id="description">Details about the device used to measure the pulse.</items>
                </term_definitions>
                <term_definitions code="at0.5">
                    <items id="text">Character</items>
                    <items id="description">The character of the pulse.</items>
                    <items id="comment">Coding with a terminology is desired, where possible. For example: full, thready, bounding, slow rising, or collapsing. Multiple terms may be recorded.</items>
                </term_definitions>
